{
  "name": "webpack-react",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "engines": {
    "node": ">=14.0.0",
    "npm": "^6.14.8"
  },
  "scripts": {
    "check-installed": "check-node-version --package && installed-check",
    "dev": "npm run check-installed && node scripts/run --open",
    "build": "npm run check-installed && node scripts/run build --release",
    "build-stats": "node scripts/run build --release --analyse",
    "lint": "npm run check-installed && npm run eslint && npm run stylelint && echo 'all lint passed'",
    "eslint": "eslint --ext .js,.ts,.tsx,.json src/ --config .eslintrc.js --quiet",
    "lint-fix": "eslint --fix .js,.ts,.tsx,.json src/ --config .eslintrc.js",
    "stylelint": "stylelint \"src/**/*.css\" \"src/**/*.less\" \"src/**/*.sass\" \"src/**/*.scss\"",
    "postinstall": "node scripts/run clean",
    "precommit": "lint-staged",
    "pack": "NODE_ENV=production npm pack",
    "test": "npm run lint && echo \"Just test lint now\"",
    "script:pack": "npm install --unsafe-perm --allow-root && npm run test && npm run build && npm run pack",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -c .commitlintrc.js -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.{ts,tsx,js}": [
      "eslint -c .eslintrc.js"
    ],
    "*.{css,less,scss}": [
      "stylelint --config .stylelintrc.json"
    ],
    "*.{ts,tsx,js,json,html,yml,css,less,scss,md}": [
      "prettier --write"
    ]
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
